<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.gui.Main">
   <children>
      <ListView fx:id="logListView" layoutX="22.0" layoutY="14.0" onMouseClicked="#listViewClick" prefHeight="312.0" prefWidth="441.0" />
      <Button fx:id="sendBtn" layoutX="469.0" layoutY="345.0" mnemonicParsing="false" onAction="#chatClick" text="send" />
      <TextArea fx:id="chatTxt" layoutX="22.0" layoutY="333.0" onKeyReleased="#txtChatPress" prefHeight="109.0" prefWidth="441.0" />
      <Button fx:id="uploadBtn" layoutX="523.0" layoutY="345.0" mnemonicParsing="false" onAction="#uploadClick" text="upload" />
      <Button fx:id="exitBtn" layoutX="469.0" layoutY="388.0" mnemonicParsing="false" onAction="#exitClick" text="exit" />
   </children>
</AnchorPane>
